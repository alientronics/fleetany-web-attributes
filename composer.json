{
  "name": "alientronics/fleetany-web-attributes",
  "description": "Attributes package for Fleetany Web",
  "license": "MIT",
  "authors": [
    {
      "name": "Alientronics"
    }
  ],
  "repositories": [
	{
		"type": "package",
		"package": {
			"version": "dev-master",
			"name": "alientronics/fleetany-attributes",
			"source": {
				"url": "https://github.com/alientronics/fleetany-attributes.git",
				"type": "git",
				"reference": "master"
			}
		}
	}
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "^5.2",
    "alientronics/fleetany-attributes": "dev-master"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~4.0",
    "laravel/framework": "5.2.*",
    "laravel/laravel": "*",
    "squizlabs/php_codesniffer": "^2.3",
    "phploc/phploc": "*",
    "sebastian/phpcpd": "*",
    "phpmd/phpmd": "*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
	"classmap": [
		"vendor/alientronics/fleetany-attributes/"
	],
	"psr-4": {
      "Alientronics\\FleetanyWebAttributes\\": "src/Alientronics/FleetanyWebAttributes"
    }
  },
  "autoload-dev": {
	"classmap": [
		"tests/",
		"vendor/alientronics/fleetany-attributes/database/"
	],
    "psr-4": {
      "App\\": "vendor/laravel/laravel/app/",
      "Tests\\": "tests/"
    }
  },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('src/routes.php', 'vendor/laravel/laravel/app/Http/routes.php');\"",
            "php -r \"copy('tests/config-app.php', 'vendor/laravel/laravel/config/app.php');\"",
            "mkdir vendor/laravel/laravel/resources/views/layouts/",
            "php -r \"copy('tests/config-app.php', 'vendor/laravel/laravel/resources/views/layouts/login.blade.php');\""
        ]
    },
  "minimum-stability": "dev"
}