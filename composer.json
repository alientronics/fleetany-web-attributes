{
  "name": "alientronics/fleetany-web-attributes",
  "description": "Attributes package for Fleetany Web",
  "license": "MIT",
  "authors": [
    {
      "name": "Alientronics"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "^5.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~4.0",
    "laravel/framework": "5.2.*",
    "laravel/laravel": "*",
    "squizlabs/php_codesniffer": "^2.3",
    "phploc/phploc": "*",
    "sebastian/phpcpd": "*",
    "phpmd/phpmd": "*",
    "codeclimate/php-test-reporter": "dev-master"
  },

  "autoload-dev": {
	"classmap": [
		"tests/"
	],
    "psr-4": {
      "App\\": "vendor/laravel/laravel/app/",
      "Tests\\": "tests/"
    }
  },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('src/routes.php', 'vendor/laravel/laravel/app/Http/routes.php');\"",
            "php -r \"copy('tests/config-app.php', 'vendor/laravel/laravel/config/app.php');\"",
            "mkdir vendor/laravel/laravel/resources/views/layouts/",
            "php -r \"copy('tests/config-app.php', 'vendor/laravel/laravel/resources/views/layouts/default.blade.php');\""
        ]
    },
  "minimum-stability": "dev"
}